<!-- JavaScript pour configurer Owl Carousel -->
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="https://owlcarousel2.github.io/OwlCarousel2/assets/owlcarousel/owl.carousel.min.js"></script>
<script>
	$(document).ready(function(){
		$(".owl-carousel").owlCarousel({
			items: 5,  // Afficher 5 éléments à la fois
			loop: true,  // Pour boucler les slides
			autoplay: true,  // Faire défiler automatiquement
			autoplayTimeout: 3000,  // Temps entre chaque slide
			margin: 10,  // Espace entre les images
			responsive: {
				0: {
					items: 1  // Sur petits écrans, on affiche 1 image à la fois
				},
				600: {
					items: 3  // Sur les écrans de taille moyenne, afficher 3 images
				},
				1000: {
					items: 5  // Sur les grands écrans, afficher 5 images
				}
			},
			nav: false,  // Désactive les flèches de navigation
			dots: false  // Désactive les points de navigation en bas
		});
	});
</script>

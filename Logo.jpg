<script>
	const scrollBtn = document.getElementById('scrollBtn');
	const arrow = document.getElementById('arrow');

	function updateArrowAndVisibility() {
		const scrollY = window.scrollY;
		const atTop = scrollY <= 10;
		const atBottom = window.innerHeight + scrollY >= document.body.scrollHeight - 10;

		if (atTop || atBottom) {
			scrollBtn.classList.remove('visible');
		} else {
			scrollBtn.classList.add('visible');
		}

		// Changer la direction de la flèche
		if (scrollY > 100) {
			arrow.classList.remove('fa-arrow-down');
			arrow.classList.add('fa-arrow-up');
		} else {
			arrow.classList.remove('fa-arrow-up');
			arrow.classList.add('fa-arrow-down');
		}
	}

	// Clic sur le bouton : aller en haut ou en bas
	scrollBtn.addEventListener('click', () => {
		if (window.scrollY > 100) {
			window.scrollTo({ top: 0, behavior: 'smooth' });
		} else {
			window.scrollTo({ top: document.body.scrollHeight, behavior: 'smooth' });
		}
	});

	// Détection immédiate
	window.addEventListener('scroll', updateArrowAndVisibility);
	window.addEventListener('mousemove', updateArrowAndVisibility);
	window.addEventListener('keydown', updateArrowAndVisibility);
	window.addEventListener('load', updateArrowAndVisibility);
</script>

<script>
	document.addEventListener("DOMContentLoaded", function () {
